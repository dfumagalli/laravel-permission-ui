{
    "name": "dfumagalli/laravel-permission-ui",
    "description": "A Laravel package for adding UI for spatie/Laravel-permission package.",
    "type": "library",
    "homepage": "https://github.com/dfumagalli/laravel-permission-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Povilas Korop",
            "email": "povilas@laraveldaily.com"
        },
        {
            "name": "Dario Fumagalli",
            "email": "dfumagalli71@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "dfumagalli\\PermissionsUI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dfumagalli\\PermissionsUI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "dfumagalli\\PermissionsUI\\PermissionsUIServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.3.1"
}
